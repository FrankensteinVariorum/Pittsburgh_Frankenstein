            <seg xml:id="C23_app601-f1823_start"/>whatever <seg xml:id="C23_app601-f1823_end"/>
            <seg type="invariant-MissingWit" xml:id="C23_app602-f1823_start"/>cause they <seg xml:id="C23_app602-f1823_end"/>might <seg xml:id="C23_app604-f1823_start"/>spring.&lt;p loc="novel1_letter4_chapter16_p475__End"/&gt;&lt;p
            loc="novel1_letter4_chapter16_p476__Start"/&gt;For <seg xml:id="C23_app604-f1823_end"/>
            <seg xml:id="C23_app605-f1823_start"/>myself, <seg xml:id="C23_app605-f1823_end"/>I <seg xml:id="C23_app607-f1823_start"/>was passive in all their <seg xml:id="C23_app607-f1823_end"/>
