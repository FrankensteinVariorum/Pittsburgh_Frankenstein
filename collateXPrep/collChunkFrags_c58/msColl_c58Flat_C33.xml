        <surface xmlns:mith="http://mith.umd.edu/sc/ns1#" lrx="5410" lry="6660" partOf="ox-frankenstein_notebook_c1.xml#ox-frankenstein_volume_iii" ulx="0" uly="0" mith:folio="24r" mith:shelfmark="MS. Abinger c. 58" xml:base="ox-ms_abinger_c58/ox-ms_abinger_c58-0047.xml" xml:id="ox-ms_abinger_c58-0047" sID="ox-ms_abinger_c58-0047"/>
